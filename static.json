{
  "root": "build/",
  "clean_urls": false,
  "https_only": true,

  "routes": {
    "/": "index.html"
    },

    "proxies": {
      "/api/": {
        "origin": "https://${API_APP_NAME}.herokuapp.com/"}
      },

      "error_page": "errors/error.html",

      "redirects": {
        "/": {
          "url": "${NEW_SITE_DOMAIN}/new/here/"
        }
      }
    
  }